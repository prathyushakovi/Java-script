<!DOCTYPE html>
<html>
	<head></head>
	<body>
		<h2>java script switch statement</h2>
		<script>
			var n =87;
			switch(true)
			{
				case n>90: {
					document.write("a grade");
					break;
				}
				case n>80: {
					document.write("b grade");
					break;
				}
				case n>70:
				{
					document.write("c grade");
					break;
				}
			    default:
				{
					document.write("hello");
					break;
				}

			}
		</script>
	</body>
</html>
